<div class="contenido">
	<div class="desplazador">
		
		<div class="contenedor">
		<div class="caja_formularios_archivos">
			
				<h2>INSERTAR DATOS</h2>
			<form action="/mostrarDatosP" method="POST" autocomplete="off">
                {{#each tabla}}
				<input type="hidden" name="id" value="{{id}}">
				<input type="hidden" name="usuario" value="{{usuario}}">
				<span>NOMBRE: {{nombre}}</span>
				<input type="text" name="nombre" placeholder="NUEVO NOMBRE" required>
				<span>APELLIDO: {{apellido}}</span>
				<input type="text" name="apellido" placeholder="NUEVO APELLIDO" required>
				<span>FECHA DE NACIMIENTO: {{fechanacimiento}}</span>				
            	<input type="date" name="fechanacimiento" placeholder="FECHA DE NACIMIENTO" required>            
				<span>SEXO: {{sexo}}
				</span>
				<select name="sexo" required>
					<option value="PORCOMPLETAR">SEXO</option >
					<option value="FEMENINO">FEMENINO</option>
					<option value="MASCULINO">MASCULINO</option>
				</select>
				<span>ESTADO CIVIL: {{ecivil}}</span>
				<select name="ecivil" required>
					<option value="PORCOMPLETAR">ESTADO CIVIL</option >
					<option value="SOLTRO">SOLTRO</option>
					<option value="CASADO">CASADO</option>
				</select>
				<span>HIJOS: {{hijos}}</span>
				<select name="hijos" required>
					<option value="PORCOMPLETAR">TIENE HIJOS</option >
					<option value="SI">SI</option>
					<option value="NO">NO</option>
				</select>				
				<span>NUMERO DE HIJOS: {{nhijos}}</span>
				<input type="number" name="nhijos" placeholder="NUEVO DATO" required>
				<span>PROFESION: {{profesion}}</span>
				<input type="text" name="profesion" placeholder="NUEVO DATO" required>
				<span>TELEFONO: {{telefono}}</span>
				<input type="text" name="telefono" placeholder="NUEVO DATO" required>
				<span>CORREO: {{email}}</span>
            	<input type="text" name="email" placeholder="EMAIL" required>
				<input type="submit" value="ENVIAR">
                 {{else}}
                    <h1>NO HAY INSTRUCTOR CON ESE USUARIO</h1>
                {{/each}}
			</form>
		</div>
		</div>		
	</div>
</div>	

{{/if}}